{
	"schemaVersion": 1,
	"id": "cinnamon",
	"version": "${version}",
	"name": "CinnamonClient",
	"description": "A Minecraft client modification that enhances your gameplay experience with additional features and optimizations.",
	"authors": [
		"Cinnamonpuma"
	],
	"contact": {
		"homepage": "https://github.com/Cinnamonpuma/CinnamonClient",
		"sources": "https://github.com/Cinnamonpuma/CinnamonClient",
		"issues": "https://github.com/Cinnamonpuma/CinnamonClient/issues"
	},
	"license": "Creative Commons Attribution-NonCommercial-ShareAlike 4.0 International License",
	"icon": "assets/cinnamon/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "code.cinnamon.Cinnamon",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"cinnamon.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}